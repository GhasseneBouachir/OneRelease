package tpjava;

import java.util.*;

public class Tp2tlc {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		String dataIn = s.nextLine();
		int state = 1;
		for(int i=0; i <dataIn.length();i++)
		{	switch(state)
				{	case(1):
						{
							if(dataIn.charAt(i) == '1')state = 2;
						}
					break;
					case(2):
						{
							if(dataIn.charAt(i) == '0')state = 3;
						}
					break;
					case(3):
						{
							if(dataIn.charAt(i) == '0')state = 4;
							if(dataIn.charAt(i) == '1')state = 5;
						}
					break;
					case(4):
						{
						if(dataIn.charAt(i) == '0')state = 5;
						}
					break;
				}
		}
		if(state==2 || state==4) System.out.println("mot acceptée");
		else System.out.println("mot réfusée");

	}
	
}
